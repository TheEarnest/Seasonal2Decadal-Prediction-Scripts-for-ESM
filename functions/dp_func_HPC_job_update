#! /bin/bash
###############################################################################
sed -i s/"#SBATCH --account=".*/"#SBATCH --account=r_SNESMt01"/g  ${VERSION}${mem}.${machine}.run
sed -i s/"#SBATCH --job-name=".*/"#SBATCH --job-name=${CPUACCOUNT}"/g ${VERSION}${mem}.${machine}.run
sed -i s/"#SBATCH --time=".*/"#SBATCH --time==00:59:00"/g ${VERSION}${mem}.${machine}.run
###############################################################################
